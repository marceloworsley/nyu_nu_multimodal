<?xml version="1.0" ?>
<options>
    <item name="device" type="CHAR" num="1024" value="ALSA - sysdefault" lock="true" help="audio device name (empty for default)" />
    <item name="api" type="INT" num="1" value="-1" lock="true" help="preferred audio api (all=-1, DS=1, MME=2, ASIO=3, WASAPI=13 ...)" />
